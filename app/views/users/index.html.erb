<div class="centered">
  <div class="centered_content">
  <h1>Listing users</h1>
<table>
  <tr>
    <th>Email</th>
    <th>Password</th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.email %></td>
    <td><%= user.password %></td>>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New User', new_user_path %>

</div>
</div>
